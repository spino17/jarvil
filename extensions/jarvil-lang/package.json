{
  "name": "jarvil-lang",
  "displayName": "jarvil",
  "description": "Syntax highlighting for jarvil",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "jarvil",
      "aliases": ["Jarvil", "jarvil"],
      "extensions": [".jv"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "jarvil",
      "scopeName": "source.jarvil",
      "path": "./syntaxes/jarvil.tmLanguage.json"
    }]
  }
}
